USERLIBS += yuos

$(OBJDIR)/user/%.o: user/%.c
	@echo + cc[USER] $<
	@mkdir -p $(@D)
	@$(CC) -nostdinc $(USER_CFLAGS) -c -o $@ $<

$(OBJDIR)/usr/%: $(OBJDIR)/user/%.o $(OBJDIR)/lib/entry.o user/user.ld
	@echo + ld $@
	@$(LD) -o $@ $(ULDFLAGS) $(LDFLAGS) -nostdlib $(OBJDIR)/lib/entry.o $@.o -L$(OBJDIR)/lib $(GCC_LIB)